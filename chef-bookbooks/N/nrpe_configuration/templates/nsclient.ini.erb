# nsclient.ini Generated by Chef for <%= node['fqdn'] %> do not edit!

<% node['nrpe_configuration']['settings'].each do |option, value| -%>
    <% if value.nil? %>
<%= option %>
    <% else %>
        <% if value.is_a? String %>
<%= option %> = <%= value %>
        <% else %>
            <% case value %>
            <% when TrueClass, FalseClass %>
<%= option %> = <%= value ? 'true' : 'false' %>
            <% else %>
<%= option %> = <%= value %>
            <% end %>
        <% end %>
    <% end %>
<% end -%>