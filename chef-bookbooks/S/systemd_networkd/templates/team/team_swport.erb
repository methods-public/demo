[Match]
Name=<%= @DevName %>

<% if node[ "SystemdNetworkd" ][ "Teams" ][ @DevName ].include?( "Attributes" ) %>
[SWPortAttrs]
  <% node[ "SystemdNetworkd" ][ "Teams" ][ @DevName ][ "Attributes" ].each do |attr,val| %>
    <% node[ "SystemdNetworkdSwitchport" ][ "TeamSupportedAttributes" ].each do |supported_attr| %>
      <% if attr == supported_attr %>
<%= attr %>=<%= val %>
      <% end %>
    <% end %>
  <% end %>
<% end %>

<% if node[ "SystemdNetworkd" ][ "Teams" ][ @DevName ].include?( "Vlans" ) %>
  <% node[ "SystemdNetworkd" ][ "Teams" ][ @DevName ][ "Vlans" ].each do |vlanid,vlanopts| %>
[Vlan]
    <% vlanopts.each do |opt,val| %>
<%= opt %>=<%= val %>
    <% end %>
  <% end %>
  
<% end %>
