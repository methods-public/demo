init_config:
  min_collection_interval: <%= @varnishstat_collection_interval %>
instances:
  # The full path to the varnishstat binary
  - varnishstat: <%=  @varnishstat_path -%>
<%- if @varnishstat_name -%> 
    # The (optional) name will be used in the varnishstat command for the
    # -n argument.
    name: <%=  @varnishstat_name -%>
<%- end -%> 
