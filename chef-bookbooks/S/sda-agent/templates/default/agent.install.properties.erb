# Installation directory of the agent
locked/agent.home=<%= node['sda-agent']['agent_dir'] %>

# Path to the Java installation
serenaRA/java.home=<%= node['sda-agent']['java_home'] %>

# Name of the agent (should be unique)
locked/agent.name=<%= node['sda-agent']['agent_name'] %>

# IP or host of the server or relay to connect to
locked/agent.jms.remote.host=<%= node['sda-agent']['remote_host'] %>
locked/agent.jms.remote.port=<%= node['sda-agent']['remote_port'] %>

# IP or host of the relay
<% if node['sda-agent']['proxy_host'] == "" -%>
#locked/agent.http.proxy.host=<%= node['sda-agent']['proxy_host'] %>
#locked/agent.http.proxy.port=<%= node['sda-agent']['proxy_port'] %>
<% else -%>
locked/agent.http.proxy.host=<%= node['sda-agent']['proxy_host'] %>
locked/agent.http.proxy.port=<%= node['sda-agent']['proxy_port'] %>
<% end -%>

# If the agent should verify the certificate of the server or relay it connects to
locked/agent.mutual_auth=<%= node['sda-agent']['mutual_auth'] %>



