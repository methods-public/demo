# NOTE: This file is controlled by chef templates!
#   Do not edit or change this file but change the following:
#    * template file: <%= @template_file %>
#    * recipe file: <%= @recipe_file %>
# --------------------------------------------------------------

[default]
  <% @config['default'].each do |k,v| -%>
  <%= k %> = <%= v %>
  <% end -%>

[chef]
  <% @config['chef'].each do |k,v| -%>
  <%= k %> = <%= v %>
  <% end -%>

[community]
  <% @config['community'].each do |k,v| -%>
  <%= k %> = <%= v %>
  <% end -%>

<% unless @config['supermarket'].nil? -%>
[supermarket]
  <% @config['supermarket'].each do |k,v| -%>
  <%= k %> = <%= v %>
  <% end -%>
<% end -%>

<% unless @config['chefclients'].nil? -%>
[chefclients]
  <% @config['chefclients'].each do |k,v| -%>
  <%= k %> = <%= v %>
  <% end -%>
<% end -%>

<% unless @config['tests'].nil? -%>
[tests]
  <% @config['tests'].each do |k,v| -%>
  <%= k %> = <%= v %>
  <% end -%>
<% end -%>

<% unless @config['git'].nil? -%>
<% @config['git'].each do |k,v| -%>
[git "<%= k %>"]
  <% v.each do |subk,subv| -%>
  <%= subk %> = <%= subv %>
  <% end -%>

<% end -%>
<% end -%>

<% if (@config['chef']['type'] == "enterprise" || @config['chef']['version'] > 11) && !@config['customers'].nil? -%>
<% @config['customers'].each do |k,v| -%>
[customer "<%= k %>"]
  <% v.each do |subk,subv| -%>
  <%= subk %> = <%= subv %>
  <% end -%>

<% end -%>
<% end -%>
