---
settings:
  check-updates: <%= @check_updates %>
  check-interval: <%= @check_interval %>
  default-executor: <%= @default_executor %>
  artifacts-dir: <%= @artifacts_dir %>

<% if @jmeter_support || @gatling_support %>
modules:
<% end %>
<% if @jmeter_support %>
  jmeter:
    path: <%= @jmeter_path %>
<% end %>
<% if @gatling_support %>
  gatling:
    path: <%= @gatling_path %>
<% end %>
