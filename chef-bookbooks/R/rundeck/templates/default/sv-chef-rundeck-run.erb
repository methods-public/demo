#!/bin/sh
exec 2>&1
exec 1> <%=@options[:log_dir]%>/server.log
exec \
chpst -u <%=@options[:user]%> /opt/chef/embedded/bin/chef-rundeck -c <%=@options[:chef_config]%> -f <%=@options[:project_config]%> -w <%=@options[:chef_webui_url]%> -o <%=@options[:chef_rundeck_host]%> -p <%=@options[:chef_rundeck_port]%> -t <%= @options[:chef_rundeck_cachetime] %> <% if @chef_rundeck_partial_search %>--partial-search true<% end %>
