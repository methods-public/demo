<% unless @default_creds.nil? %>
[default]
aws_access_key_id = <%= @default_creds['aws_access_key_id'] %>
aws_secret_access_key = <%= @default_creds['aws_secret_access_key'] %>
<% end %>
<% @creds.sort.each do |i, v| %>
[profile <%= i %>]
aws_access_key_id = <%= v['aws_access_key_id'] %>
aws_secret_access_key = <%= v['aws_secret_access_key'] %>
<% end %>
