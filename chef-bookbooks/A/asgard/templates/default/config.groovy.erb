grails {
	awsAccounts=[ <%= node['asgard']['aws']['accounts'].join(', ') %> ]
	awsAccountNames=[ <%= node['asgard']['aws']['accountNames'].keys.collect {|id| "'#{id}':'#{node['asgard']['aws']['accountNames'][id]}'" }.join(', ') %> ]
}
secret {
	accessId='<%= node["asgard"]["aws"]["accessId"] %>'
	secretKey='<%= node["asgard"]["aws"]["secretKey"] %>'
}
cloud {
	accountName='<%= node["asgard"]["cloud"]["accountName"] %>'
	publicResourceAccounts=[]
}
