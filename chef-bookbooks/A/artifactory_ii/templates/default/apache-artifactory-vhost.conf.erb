<VirtualHost *:80>
	ServerName <%= @host_name %>
	ProxyPass / http://localhost:<%=node['artifactory_ii']['port'] %>/
	ProxyPassReverse / http://localhost:<%=node['artifactory_ii']['port'] %>/
	ProxyRequests Off
	ProxyPreserveHost on
	
	<Proxy http://localhost:<%=node['artifactory_ii']['port'] %>/*>
    	Order deny,allow
    	Allow from all
	</Proxy>

</VirtualHost>
