[Unit]
Description = Hadoop YARN ProxyServer.
After = syslog.target network.target remote-fs.target

[Service]
User = <%= node.apache_hadoop.yarn.user %>

PIDFile = /tmp/yarn-<%= node.apache_hadoop.yarn.user %>-proxyserver.pid
ExecStart = <%= node.apache_hadoop.base_dir %>/sbin/start-ps.sh
ExecStop = <%= node.apache_hadoop.base_dir %>/sbin/stop-ps.sh
ExecReload = <%= node.apache_hadoop.base_dir %>/sbin/restart-ps.sh

Type = forking

[Install]
WantedBy = multi-user.target
