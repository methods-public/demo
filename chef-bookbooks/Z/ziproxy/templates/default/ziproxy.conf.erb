# Generated by Chef - local changes will be overwritten

Port = <%= node['ziproxy']['port'] %>
Address = "<%= node['ziproxy']['address'] %>"
RunAsUser = "<%= node['ziproxy']['user'] %>"
RunAsGroup = "<%= node['ziproxy']['group'] %>"
DebugLog = "<%= node['ziproxy']['log_dir'] %>/debug.log"
ErrorLog = "<%= node['ziproxy']['log_dir'] %>/error.log"
AccessLog = "<%= node['ziproxy']['log_dir'] %>/access.log"
<% if node['ziproxy']['next_proxy'] == true -%>
NextProxy = "<%= node['ziproxy']['next_proxy']['address'] %>"
NextPort = <%= node['ziproxy']['next_proxy']['port'] %>
<% end -%>
TransparentProxy = <%= node['ziproxy']['transparent'] %>
OverrideAcceptEncoding = <%= node['ziproxy']['override_accept_encoding'] %>
ConventionalProxy = <%= node['ziproxy']['conventional_proxy'] %>
MaxSize = <%= node['ziproxy']['max_size'] %>
UseContentLength = <%= node['ziproxy']['use_content_length'] %>
Gzip = <%= node['ziproxy']['gzip'] %>
LosslessCompressCT = {
  "text/*",
  "application/asp",
  "application/awk",
  "application/cgi",
  "application/class",
  "application/css",
  "application/dvi",
  "application/executable",
  "application/font",
  "application/futuresplash",
  "application/iso9660-image",
  "application/java",
  "application/javascript",
  "application/json",
  "application/msexcel",
  "application/mspowerpoint",
  "application/msword",
  "application/pdf",
  "application/perl",
  "application/php",
  "application/postscript",
  "application/python",
  "application/rtf",
  "application/shellscript",
  "application/shockwave",
  "application/staroffice",
  "application/tar",
  "application/truetype-font",
  "application/vnd.*",
  "application/*+xml",
  "application/xml",
  "application/xml-dtd",
  "image/svg+xml"
}
AllowLookChange = <%= node['ziproxy']['allow_look_change'] %>
ConvertToGrayscale = <%= node['ziproxy']['convert_to_grayscale'] %>
ProcessJPG = <%= node['ziproxy']['process_jpg'] %>
ProcessPNG = <%= node['ziproxy']['process_png'] %>
ProcessGIF = <%= node['ziproxy']['process_gif'] %>

ImageQuality = {<%= node['ziproxy']['compression']['rate'] %>}
