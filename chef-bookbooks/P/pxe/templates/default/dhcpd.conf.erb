default-lease-time 600;
max-lease-time 7200;
subnet <%= @config['subnet'] %> netmask <%= @config['subnet_mask'] %> {
    range <%= @config['dynamic_range'] %>;
    option subnet-mask <%= @config['subnet_mask'] %>;
    option routers <%= @config['routers'] %>;
    option broadcast-address <%= @config['broadcast'] %>;
    filename "pxelinux.0";
    next-server <%= @config['next_server'] %>;
}